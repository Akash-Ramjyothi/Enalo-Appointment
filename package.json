{
  "homepage": "https://Akash-Ramjyothi.github.io/Enalo-Appointment",
  "name": "enalo-form",
  "version": "1.0.0",
  "private": false,
  "description": "Enalo Appointment Booking Form built with React, TypeScript and Ant Design",
  "author": "Akash Ramjyothi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akash-Ramjyothi/Enalo-Appointment.git"
  },
  "keywords": [
    "react",
    "typescript",
    "appointment-booking",
    "ant-design",
    "gh-pages",
    "form-validation"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "antd": "^5.20.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.105",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "gh-pages": "^6.1.1",
    "typescript": "^4.9.5",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --watchAll=false",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "clean": "rimraf build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions"
    ]
  }
}
